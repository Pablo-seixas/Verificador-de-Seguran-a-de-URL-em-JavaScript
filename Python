# Leitura da entrada do usu√°rio:
url = input()
resultado = ""

if url.startswith("https://"):
    resultado = "URL segura"
elif url.startswith("http://"):
    resultado = "URL nao segura"
else:
    resultado = "Formato invalido"

# Exibe o resultado
print(resultado)  # Imprime o dado, pulando linha
